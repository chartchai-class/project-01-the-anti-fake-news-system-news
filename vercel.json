{
  "version": 2,
  "builds": [
    {
      "src": "LegitNews/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "LegitNews/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "LegitNews/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "LegitNews/$1"
    }
  ]
}
